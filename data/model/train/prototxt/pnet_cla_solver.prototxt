#PNet
net: "prototxt/pnet_cla.prototxt"

test_iter: 100
test_interval: 100

#set the learning rate
base_lr: 0.05
momentum: 0.9
weight_decay: 0.00005

#learning rate policy
#lr_policy: "step"
#stepsize: 2000
#gamma: 0.01
lr_policy: "inv"
power: 0.75
gamma: 0.00001

#display
display: 100
max_iter: 10000

snapshot: 100
snapshot_prefix: "model/pnet/pnet_cla"

solver_mode: GPU
