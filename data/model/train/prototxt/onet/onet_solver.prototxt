#PNet
net: "prototxt/pnet.prototxt"

test_iter: 100
test_interval: 500

#set the learning rate
base_lr: 0.00000675
momentum: 0.9
weight_decay: 0.008

#learning rate policy
lr_policy: "inv"
gamma: 0.0001
power: 0.75

#display
display: 500
max_iter: 500000

snapshot: 5000
snapshot_prefix: "model/pnet"

solver_mode: GPU